PROGRAM p
BEGIN

    INT a,b,k,g,p,u;
    STRING sp := " ";

    FUNCTION VOID main()
    BEGIN

        b := 1;
            a := 1;
            FOR( ; a <= 120; ) 
            IF (b = 1)
                g := 1*a;
                WRITE (g,sp);
            ELSE
                IF (b = 2)
                    p := 2*a;
                    WRITE (p,sp);
                ELSE
                    IF (b = 3)
                        k := 3*a;
                           WRITE (k,sp);
                    FI
                        IF (b = 4)
                            u := 4*a;
                            WRITE (u,sp);
                        FI  
                FI        
            FI
                    b := b + 1;
                    a := a + 20;
            ROF
            RETURN 0;
    END
END