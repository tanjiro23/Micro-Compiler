PROGRAM nested
BEGIN

    INT i, j, k, result;
     
    FUNCTION VOID main()
    BEGIN
        i := 0;
        j := 0;
        k := 0;
        result := 0;

        FOR (i := 0 ; i <= 99; i := i + 1)
            FOR (j := 0 ; j <= 99; j := j + 1)
                FOR (k := 0 ; k < 100; k := k + 1)
                    result := result + 1;
                ROF
            ROF
        ROF

        WRITE (result);
    END
END